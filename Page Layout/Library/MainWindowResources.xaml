    <!--#region Style-->
    <ControlTemplate x:Key="MainListBoxStyle" TargetType="ListBox">
        <Border  BorderThickness="1" CornerRadius="19" UseLayoutRounding="True" TextBlock.Foreground="Black" TextBlock.TextAlignment="Center" Visibility="Visible">
            <Border.BorderBrush>
                <LinearGradientBrush>
                    <GradientStopCollection>
                        <GradientStop Offset="0" Color="#752F0C"></GradientStop>
                        <GradientStop Offset="1" Color="#DB5817"></GradientStop>
                    </GradientStopCollection>
                </LinearGradientBrush>
            </Border.BorderBrush>
            <Border.Background>
                <LinearGradientBrush>
                    <GradientStopCollection>
                        <GradientStop Offset="0" Color="#D9D9D9"></GradientStop>
                        <GradientStop Offset="1" Color="LightGray"></GradientStop>
                    </GradientStopCollection>
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter RecognizesAccessKey="True"></ContentPresenter>
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="NextButtonStyle" TargetType="Button">
        <Border BorderBrush="Black" BorderThickness="1" CornerRadius="19" UseLayoutRounding="True" TextBlock.Foreground="Black" TextBlock.TextAlignment="Center">
            <Border.Background>
                <LinearGradientBrush>
                    <GradientStopCollection>
                        <GradientStop Offset="0" Color="#FF7246"></GradientStop>
                        <GradientStop Offset="1" Color="#DB5817"></GradientStop>
                    </GradientStopCollection>
                </LinearGradientBrush>
            </Border.Background>
            <ContentPresenter RecognizesAccessKey="True"></ContentPresenter>
        </Border>
    </ControlTemplate>
    <!--#endregion-->
